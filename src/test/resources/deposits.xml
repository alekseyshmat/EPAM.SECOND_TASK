    <SavingDeposit>
        <nameOfBank>IDEA</nameOfBank>
        <country>UKRAINE</country>
        <profitability>10</profitability>
        <timeConstraints>45</timeConstraints>
        <currencyType>GRN</currencyType>
        <capitalization>10</capitalization>
        <cashout>true</cashout>
    </SavingDeposit>

</deposits>