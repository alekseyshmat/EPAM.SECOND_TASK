<?xml  version="1.0"  encoding="UTF-8"?>
<deposits xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://www.example.com/deposits"
          xsi:schemaLocation="http://www.example.com/deposits  schema.xsd">

    <TimeDeposit>
        <nameOfBank>Alfa</nameOfBank>
        <country>Belarus</country>
        <profitability>25</profitability>
        <timeConstraints>30</timeConstraints>
        <currencyType>USD</currencyType>
        <minimumSum>3.0</minimumSum>
        <onlineOpening>true</onlineOpening>
